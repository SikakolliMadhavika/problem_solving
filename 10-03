def reverse_list_swap(list):
    j=-1
    for i in range((len(list)//2)):
        list[i],list[j] = list[j], list[i]
        j-=1
    print(list)
reverse_list_swap([1,2,4,5,6,7])
reverse_list_swap([1,2,4,5,6,7,8])
reverse_list_swap([1])



def reverse_list_swap(list):
    copy_list=[]
    for i in range(-1,-len(list)-1,-1):
        copy_list.append(list[i])
    print(copy_list)
reverse_list_swap([1,2,4,5,6,7])
reverse_list_swap([1,2,4,5,6,7,8])
reverse_list_swap([1])

